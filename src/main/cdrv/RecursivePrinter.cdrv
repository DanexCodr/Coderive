unit sample get {
    cdrv.Math
}

share RecursivePrinter {
    share main() {
    	output "This is a test for for-loop"
    
        output "Counting up:"
        for i by 1 in 1 to 10 {
            output i
        }
        
        output "Counting down:"
        for i by -1 in 10 to 1 {
            output i
        }
    }
}

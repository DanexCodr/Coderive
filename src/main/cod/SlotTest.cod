
use {
	lang.Sys
}

share compute(a: int, b: int)
:: sum: int, diff: int
~> sum: a + b, diff: a - b

share trying(a: int, b: int)
:: int, int|float
~> a * b, a / b

share main() {
Sys.outln([sum]:compute(2, 1))
Sys.outln([diff]:compute(2, 1))
Sys.outln([0]:trying(2, 1))
Sys.outln([1]:trying(2, 4))
}
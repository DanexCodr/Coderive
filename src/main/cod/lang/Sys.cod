unit lang

share Sys {
	
	// Methods declared as builtin can have any text inside its param

    /* the printer */
    builtin out(...values)
    
    /* 'outln' is the printer that also prints line */
    builtin outln(...values)
    
    /* Input method - reads from stdin */
    builtin in( not needed if declared... type, optional... message, optional... source)
}